{
  "manifest_version": 2,
  "name": "Timer",
  "version": "1.0",
  "description": "Keep track of the time with this extension",
  "permissions": ["declarativeContent","storage"],
  "background": {
     "scripts": ["background.js"],
     "persistent": false
   },

  "browser_action": {
    "default_icon": "image.png",
    "default_title": "Timer",
    "default_popup": "popup.html"
  },

  "permissions":[
    "activeTab"
  ]


}
